  "devDependencies": {
    "@mdi/js": "7.4.47",
    "@muenchen/prettier-codeformat": "1.0.2",
    "@tsconfig/node-lts": "22.0.1",
    "@types/node": "22.10.10",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.4.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.21.0",
    "eslint-plugin-vue": "9.32.0",
