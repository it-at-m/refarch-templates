{
  "name": "refarch-frontend",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=22.14 <=22"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "build": "vue-tsc --build --noCheck && vite build && npm run post-build",
    "post-build": "cyclonedx-npm --omit dev --omit optional --output-reproducible -o dist/application.cdx.json  --validate",
    "lint": "prettier . --check && eslint . && vue-tsc --build --noEmit",
    "fix": "prettier . --write && eslint . --fix"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.5",
    "@muenchen/appswitcher-vue": "2.2.0",
    "@vueuse/core": "13.3.0",
    "pinia": "3.0.3",
    "vue": "3.5.16",
    "vue-i18n": "11.1.5",
    "vue-router": "4.5.1",
    "vuetify": "3.8.8"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "3.0.0",
    "@intlify/eslint-plugin-vue-i18n": "4.0.1",
    "@intlify/unplugin-vue-i18n": "6.0.8",
    "@mdi/js": "7.4.47",
    "@muenchen/prettier-codeformat": "1.0.2",
    "@tsconfig/node-lts": "22.0.1",
    "@types/node": "22.14.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.28.0",
    "eslint-plugin-vue": "10.2.0",
    "jsdom": "26.1.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "unplugin-fonts": "1.3.1",
    "vite": "6.3.5",
    "vite-plugin-vue-devtools": "7.7.6",
    "vite-plugin-vuetify": "2.1.1",
    "vitest": "3.2.3",
    "vue-tsc": "2.2.10"
  }
}
