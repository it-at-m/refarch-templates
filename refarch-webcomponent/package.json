{
  "name": "refarch-webcomponent",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18 <=20"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "build": "vue-tsc --build --force && vite build && npm run post-build",
    "post-build": "node ./processes/post-build.js",
    "preview": "vite preview",
    "lint": "prettier . --check && eslint . && vue-tsc --noEmit",
    "fix": "prettier . --write && eslint . --fix"
  },
  "dependencies": {
    "@muenchen/muc-patternlab-vue": "1.14.0",
    "@vueuse/core": "11.1.0",
    "vue": "3.5.12"
  },
  "devDependencies": {
    "@eslint/js": "9.12.0",
    "@muenchen/prettier-codeformat": "1.0.2",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.14.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.1.3",
    "@vue/language-server": "2.1.6",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "eslint": "9.12.0",
    "eslint-plugin-vue": "9.29.1",
    "jsdom": "25.0.1",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite": "5.4.10",
    "vitest": "2.1.3",
    "vue-tsc": "2.1.6"
  }
}
