{
  "name": "refarch-webcomponent",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20 <=22"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "build": "vue-tsc --build --noCheck && vite build && npm run post-build",
    "post-build": "node ./processes/post-build.js",
    "preview": "vite preview",
    "lint": "prettier . --check && eslint . && vue-tsc --noEmit",
    "fix": "prettier . --write && eslint . --fix"
  },
  "dependencies": {
    "@muenchen/muc-patternlab-vue": "2.0.0",
    "@vueuse/core": "12.0.0",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@muenchen/prettier-codeformat": "1.0.2",
    "@tsconfig/node-lts": "22.0.1",
    "@types/node": "22.10.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "14.1.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.15.0",
    "eslint-plugin-vue": "9.31.0",
    "jsdom": "25.0.1",
    "prettier": "3.4.1",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vite-plugin-vue-devtools": "7.6.7",
    "vitest": "2.1.6",
    "vue-tsc": "2.1.10"
  }
}
